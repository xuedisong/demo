package com.cloudhua.imageplatform.config;

/**
 * Created by yangchao on 2017/8/15.
 * 状态配置文件
 */
public class Status {
    /**
     * 照片初始化状态
     * 注：图片状态负数情况标示图片逻辑删除
     */
    public static final int IMAGE_INIT = 1;

    /**
     * 照片已经审查通过且无缺陷状态
     */
    public static final int IMAGE_ACCESS_NORMAL = 1 << 2;

    /**
     * 照片已经审查完成照片中有缺陷状态
     */
    public static final int IMAGE_ACCESS_DEFECT = 1 << 3;

    /**
     * tower表中记录标示线路信息
     */
    public static final int TOWER_LINE = 1;

    /**
     * tower表中记录标示高压塔信息
     */
    public static final int TOWER_TOWER = 1 << 2;
}
